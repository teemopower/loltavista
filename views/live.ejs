<h1 class='liveTitle'>live page</h1>
<div class='liveContainer'>
<p class="versus">VS.</p>
<% var count = 0;%>
<% sum.forEach(function(data){%>
    
    <% var array = []; %>
    <% array.push(data.championName); %>
    <% for(var i = 0; i < array.length; i ++){ %>
      <% if(data.championName == array[i]){ %>
        <img src="/images/<%= array[i].toLowerCase().replace(/[^a-zA-Z0-9+]+/gi, ''); %>.jpg" class="circle">
      <% } %> 
    <% } %>

  <% //console.log(data);%>
  <% count += 1;%>
 
    <% if (count <= 5){ %>
      <ul>
        <div class='team1'>
          <li class="teamOneName"><%= data.summonerName %></li>
        </div>
      <% } else if (count >= 6 && count <= 10){ %>
        <div class='team2'>
          <li class="teamTwoName"><%= data.summonerName %></li>
        </div>
      </ul>
    <% } %>
  
<% }); %>
</div>